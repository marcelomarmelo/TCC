@model LoginModel

<form action="" method="get">
    <label>Email</label>
    <input type="email" name="Login" value="" />
    <br />
    <label>Senha</label>
    <input type="password" name="Senha" value="" />
    <br />
    <button type="submit" class="btn btn-primary">
        Logar
    </button>
</form>

<h1>@Model.Login</h1>

<h1>@Model.Senha</h1>